<div class="page-header">
  <%= link_to tests_path, :class => 'btn btn-default' do %>
    <span class="fa fa-list-alt"></span>
    <%= _('Back') %>
  <% end %>
  <%= link_to edit_test_path(@test), :class => 'btn btn-primary' do %>
    <span class="fa fa-pencil"></span>
  <%= _('Edit') %>
  <% end %>
  <h1><%= _('Show test') %></h1>
</div>

<dl class="dl-horizontal">
  <dt><%= _('User:') %></dt>
  <dd><%= @test.user_id %></dd>
  <dt><%= _('Exam:') %></dt>
  <dd><%= @test.exam_id %></dd>
  <dt><%= _('Special:') %></dt>
  <dd><%= @test.special %></dd>
  <dt><%= _('Obtained experience:') %></dt>
  <dd><%= @test.obtained_experience %></dd>
</dl>
