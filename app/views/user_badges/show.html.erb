<div class="page-header">
  <%= link_to user_badges_path, :class => 'btn btn-default' do %>
    <span class="fa fa-list-alt"></span>
    <%= _('Back') %>
  <% end %>
  <%= link_to edit_user_badge_path(@user_badge), :class => 'btn btn-primary' do %>
    <span class="fa fa-pencil"></span>
  <%= _('Edit') %>
  <% end %>
  <h1><%= _('Show user_badge') %></h1>
</div>

<dl class="dl-horizontal">
  <dt><%= _('Unlocked date:') %></dt>
  <dd><%= @user_badge.unlocked_date %></dd>
  <dt><%= _('Badge:') %></dt>
  <dd><%= @user_badge.badge_id %></dd>
  <dt><%= _('User:') %></dt>
  <dd><%= @user_badge.user_id %></dd>
  <dt><%= _('Exam:') %></dt>
  <dd><%= @user_badge.exam_id %></dd>
  <dt><%= _('Notified:') %></dt>
  <dd><%= @user_badge.notified %></dd>
</dl>
