<div class="page-header">
  <%= link_to user_subjects_path, :class => 'btn btn-default' do %>
    <span class="fa fa-list-alt"></span>
    <%= _('Back') %>
  <% end %>
  <%= link_to edit_user_subject_path(@user_subject), :class => 'btn btn-primary' do %>
    <span class="fa fa-pencil"></span>
  <%= _('Edit') %>
  <% end %>
  <h1><%= _('Show user_subject') %></h1>
</div>

<dl class="dl-horizontal">
  <dt><%= _('User:') %></dt>
  <dd><%= @user_subject.user_id %></dd>
  <dt><%= _('Subject:') %></dt>
  <dd><%= @user_subject.subject_id %></dd>
  <dt><%= _('Role:') %></dt>
  <dd><%= @user_subject.role %></dd>
</dl>
